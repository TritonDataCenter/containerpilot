{
  "consul": "consul:8500",
  "logging": {
    "level": "DEBUG",
    "format": "text"
  },
  jobs: [
    {
      "name": "app",
      "port": 8000,
      "exec": [
        "/usr/local/bin/node",
        "/usr/local/bin/http-server", "/srv", "-p", "8000"],
      heartbeat: 1,
      "ttl": 5
    },
    {
      "name": "coprocess",
      "exec": ["/bin/coprocess.sh", "arg1"],
      "restarts": 1
    }
  ],
  health: [
    {
      name: "app",
      exec: "/usr/bin/curl --fail -s -o /dev/null http://localhost:8000",
      poll: 1,
    }
  ]
}
