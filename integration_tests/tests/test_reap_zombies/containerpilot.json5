{
  consul: "consul:8500",
  jobs: [
    {
      name: "zombies",
      port: 8000,
      exec: ["tail", "-f"],
      health: {
        exec: "/bin/sh /zombie.sh",
        interval: 1,
        ttl: 5
      },
      tags: ["application"]
    },
    {
      name: "slow-zombies",
      exec: "/bin/sh /slow-zombie.sh"
    }
  ]
}
