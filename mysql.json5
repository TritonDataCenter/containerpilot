{
  consul: "localhost:8500",
  logging: { level: "DEBUG" },
  jobs: [
    {
      name: "preStart",
      exec: "sleep 20",
      when: {
        source: "consul-agent",
        once: "healthy"
      }
    },
    {
      name: "consul-agent",
      restarts: "unlimited",
      exec: "sleep 1000",
      health: {
        exec: "sleep 1",
        interval: 5,
        ttl: 10
      }
    }
  ],
  control: {
    socket: "./containerpilot.sock"
  }
}
